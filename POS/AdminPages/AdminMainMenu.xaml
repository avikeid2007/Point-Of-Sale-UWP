<Page
    x:Class="POS.AdminMainMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:POS"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Loading="Page_Loading">

    <Grid Background="Transparent">
        <StackPanel Grid.Row="3">
            <TextBlock Text="Main Menu List" FontSize="25"/>
            <ComboBox x:Name="mainMenuList" SelectionChanged="mainMenuList_SelectionChanged">
                <ComboBoxItem Content="Open Tickets"/>
                <ComboBoxItem Content="Ready Date Not Passed"/>
                <ComboBoxItem Content="Tickets Created Today"/>
                <ComboBoxItem Content="Tickets in the Last 7 Days"/>
                <ComboBoxItem Content="Last 25 Tickets"/>
                <ComboBoxItem Content="Last 50 Tickets"/>
                <ComboBoxItem Content="No Data(removes list)"/>

            </ComboBox>

        </StackPanel>
    </Grid>
</Page>
